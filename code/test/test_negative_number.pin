// 负数
fn test1(){
	var a = -1;
	assert(a==-1);

	var b = 1-2;
	assert(b==-1);

	var c = 1+-2;
	assert(c==-1);

	var d = -2*3;
	assert(d==-6);

	var e = -1 + a;
	assert(e==-2);
}
// 负数的比较
fn test2(){
	var b = -99 > 300;
	assert(b==false);

	b = -99 > -300;
	assert(b==true);

	b = 30 <= -300;
	assert(b==false);
}
fn main() i32 {
	test1();
	test2();
	return 0;
}
