class Foo{
	id int;

	fn GetId() int{
		return this.id+3;
	}

	fn Get{
		[T ToString](a T) str {
			return a.tostring();
		}
	}
}

class Bar{
	id int;
	name str;
	foo Foo;

	fn GetId() int{
		return this.id + this.foo.GetId();
	}
	fn Print(){
		var i = this.GetId()+100;
		printf("id={} name={}\n", i, this.name);
	}
}

var foo Foo;
printf("foo.Get(1)={}\n", foo.Get(1));
printf("foo.Get(\"hes\")={}\n", foo.Get("hes"));

var bar Bar;
var q = bar.Print;
q();
