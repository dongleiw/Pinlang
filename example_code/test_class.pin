fn test1() {
	class Foo{
		a i32;
		b i32;
	}
	var f = Foo{
		a:1,
		b:2,
	};

	assert(f.a==1);
	assert(f.b==2);

	f.a=100;
	assert(f.a==100);
	assert(f.b==2);
}
fn test2() {
	class Foo{
		a i32;
		b i32;

		fn GetA() i32{
			return this.a;
		}
	}
	var f = Foo{
		a:1,
		b:2,
	};
	assert(f.a==1);
	assert(f.b==2);

	assert(f.GetA()==1);
}
fn main() i32 {
	test1();
	test2();
	return 0;
}
