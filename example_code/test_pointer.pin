// 指针
fn get_value_from_pointer(ptr *int) int{
	return *ptr;
}
fn modify_value_from_pointer(ptr *int) {
	*ptr=2;
}
fn swap_value(a *int, b *int) {
	var tmp = *a;
	*a = *b;
	*b = tmp;
}
